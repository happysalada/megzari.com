{
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "start:dev": "webpack-dev-server --mode development",
    "serve:dist": "yarn serve -s -l 8000 dist",
    "prerender": "yarn concurrently -k -s first \"yarn serve:dist\" \"wait-on http://localhost:8000/ && react-snap\"",
    "build:release": "yarn build && yarn prerender"
  },
  "reactSnap": {
    "source": "dist",
    "externalServer": true,
    "port": 8000
  },
  "devDependencies": {
    "@wasm-tool/wasm-pack-plugin": "^1.3.1",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "find": "^0.3.0",
    "html-webpack-plugin": "^4.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "no-emit-webpack-plugin": "^2.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-typed-css-classes": "^0.1.6",
    "react-snap": "^1.23.0",
    "serve": "^11.3.1",
    "style-loader": "^1.2.1",
